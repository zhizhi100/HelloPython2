{
	"Modify" : {
		"Type" : ["html"],
		"Rules" : [{
				"Action" : "AppendHTML",
				"Content" : "<script>alert(\"Hello!\");</script>",
				"MatchMode" : "StartWith",
				"MatchContent" : "dddl",
				"Type" : "html",
				"Comment" : ""
			}, {
				"Action" : "AppendHTML",
				"Content" : "<script>alert(\"Hello!\");</script>",
				"MatchMode" : "Regex",
				"MatchContent" : "dddl",
				"Type" : "html",
				"Comment" : ""
			}
		]
	},
	"Redirect" : {
		"Type" : ["*"],
		"Rules" : [{
				"Action" : "Change",
				"Content" : "http://www.hao123.com/changed.js",
				"MatchMode" : "StartWith",
				"MatchContent" : "http://www.hao123.com/test",
				"Type" : "js",
				"Comment" : ""
			}, {
				"Action" : "ChangeKeyword",
				"Content" : "test|changedkeyword",
				"MatchMode" : "HasKeyword",
				"MatchContent" : "www.HasKeyword.com",
				"Type" : "js",
				"Comment" : ""
			}, {
				"Action" : "RegexChange",
				"Content" : "test|RegexChange",
				"MatchMode" : "Regex",
				"MatchContent" : "\\S+www.RegexChange.com\\S+",
				"Type" : "js",
				"Comment" : ""
			}, {
				"Action" : "Change",
				"Content" : "http://www.hao123.com/changed.js",
				"MatchMode" : "Equal",
				"MatchContent" : "http://www.Equal.com/test.js",
				"Type" : "js",
				"Comment" : ""
			}, {
				"Action" : "Change",
				"Content" : "http://www.hao123.com/changed.js",
				"MatchMode" : "EndWith",
				"MatchContent" : "/EndWith.js",
				"Type" : "js",
				"Comment" : ""
			}
		]
	}
}
