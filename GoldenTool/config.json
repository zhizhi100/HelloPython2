{
	"Modify" : {
		"Type" : ["html"],
		"Rules" : [{
				"Action" : "AppendHTML",
				"Content" : "<script>alert(\"Hello!\");</script>",
				"MatchMode" : "StartWith",
				"MatchContent" : "http://127.0.0.1/testpage2",
				"Type" : "html",
				"Comment" : ""
			}, {
				"Action" : "ChangeKeyword",
				"Content" : "test|changedRegexword",
				"MatchMode" : "Regex",
				"MatchContent" : "\\S+Regex\\S+",
				"Type" : "html",
				"Comment" : ""
			}, {
				"Action" : "RegexChange",
				"Content" : "test|changedkeyword",
				"MatchMode" : "Equal",
				"MatchContent" : "http://127.0.0.1/Equal.html",
				"Type" : "html",
				"Comment" : ""
			}
		]
	},
	"Redirect" : {
		"Type" : ["*"],
		"Rules" : [{
				"Action" : "Change",
				"Content" : "http://localhost:8001/static/hello.html",
				"MatchMode" : "StartWith",
				"MatchContent" : "http://localhost/testpage",
				"Type" : "js",
				"Comment" : ""
			}
		]
	}
}
